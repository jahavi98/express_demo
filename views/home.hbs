<br>
<h4>{{{__ "aluser"}}}</h4>
<a class="btn btn-primary" href="/users/create">{{{__ "cruser"}}}</a><br><br>
<table class="table table-bordered" id="data">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">{{{__ "name"}}}</th>
      <th scope="col">{{{__ "uname"}}}</th>
      <th scope="col">{{{__ "act"}}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.name}}</td>
      <td>{{this.username}}</td>
      <td>
        <a class="btn btn-success" href="/users/edit/{{this.id}}">Edit</a>
        <a class="btn btn-primary" href="/users/delete/{{this.id}}">Delete</a>
      </td>
    </tr>
   {{/each}}
  </tbody>
</table>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
<script>
  jQuery(document).ready( function () {
    jQuery('#data').DataTable();
} );
</script>