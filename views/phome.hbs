
<br>
<h4> {{{__ "allpro"}}}</h4>
<a class="btn btn-primary" href="/products/pcreate">{{{__ "cproduct"}}}</a><br> <br>
<a class="btn btn-primary" href="/products/download">{{{__ "csv"}}}</a>
<br>
<br>
<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">{{{__ "pname"}}}</th>
      <th scope="col">{{{__ "pnumber"}}}</th>
      <th scope="col">{{{__ "desc"}}}</th>
      <th scope="col">{{{__ "pimg"}}}</th>
      <th scope="col">{{{__ "category"}}}</th>
      <th scope="col">{{{__ "price"}}}</th>
      <th scope="col">{{{__ "strat_date"}}}</th>
      <th scope="col">{{{__ "end_date"}}}</th>
      <th scope="col">{{{__ "status"}}}</th>
      <th scope="col">{{{__ "act"}}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.name}}</td>
      <td>{{this.pnumber}}</td>
      <td>{{this.description}}</td>
      <td>{{this.image}}</td>
      <td>{{this.category}}</td>
      <td>{{this.price}}</td>
      <td>{{this.start_date}}</td>
      <td>{{this.end_date}}</td>
      <td>{{this.status}}</td>
      <td>
        <a class="btn btn-success" href="/products/pedit/{{this.id}}">{{{__ "edit"}}}</a><br> <br>
        <a class="btn btn-primary" href="/products/delete/{{this.id}}">{{{__ "dlt"}}}</a><br> <br>
      </td>
    </tr>
   {{/each}}
  </tbody>
</table>